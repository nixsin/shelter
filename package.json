{
  "name": "shelter",
  "version": "0.1.0",
  "description": "Boilerplate React/Babel/Webpack/Kraken app",
  "author": "nitsingh@paypal.com",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf .build",
    "lint": "eslint . --ext .jsx --ext .js",
    "start": "node server.js",
    "tar": "tar -cf $(date +'%m-%d-%Y').tar ./node_modules",
    "buildcss": "mkdir .build/css && lessc public/css/app.less .build/css/app.css",
    "build": "npm run clean && webpack && npm run buildcss",
    "nuke": "npm run tar; rm -rf node_modules && npm install && npm run build",
    "nuke-all": "npm cache clean; npm run nuke"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "supertest": "^2.0.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "express": "^4.14.0",
    "kraken-js": "^2.1.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-engine": "^4.1.0",
    "react-router": "^2.6.1"
  }
}
