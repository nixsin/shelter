{
  "name": "shelter",
  "version": "0.1.0",
  "description": "Basic React/Babel/Webpack/Kraken application",
  "author": "nitsingh@paypal.com",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf .build",
    "start": "node server.js",
    "tar": "tar -cf $(date +'%m-%d-%Y').tar ./node_modules",

    "build": "npm run clean && webpack",
    "nuke": "npm run tar; rm -rf node_modules && npm install && npm run build",
    "nuke-all": "npm cache clean; npm run nuke"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "json-loader": "^0.5.4",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "express": "^4.14.0",
    "kraken-js": "^2.1.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-engine": "^4.1.0",
    "react-router": "^2.6.1"
  }
}
