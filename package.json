{
  "name": "shelter",
  "version": "0.1.0",
  "description": "Boilerplate React/Babel/Webpack/Kraken app",
  "author": "nitsingh@paypal.com",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf .build",
    "lint": "eslint . --ext .jsx --ext .js",
    "pack": "time( mkdir .modules; tar cf ./.modules/node_modules-$(git rev-parse --abbrev-ref HEAD).tar node_modules )",
    "unpack": "time ( tar xf ./.modules/node_modules-$(git rev-parse --abbrev-ref HEAD).tar && npm prune && npm install );  npm outdated;",
    "checkpoint": "npm run pack; npm shrinkwrap --dev",
    "build": "npm run clean && webpack",
    "nuke": "time( rm -rf node_modules && npm i && npm run build ); npm outdated;",
    "nuke-all": "time( npm cache clean && npm run nuke )"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      [
        "resolver",
        {
          "resolveDirs": [
            "public"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "pre-commit": "^1.1.3",
    "supertest": "^2.0.0"
  },
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-resolver": "0.0.7",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "kraken-js": "^2.1.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "progress-bar-webpack-plugin": "^1.9.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-engine": "^4.1.0",
    "react-router": "^2.6.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "pre-commit": [
    "lint",
    "build"
  ]
}
